package br.atos.acelera.dados;

public class Aluno extends Pessoa{
	
	void estudar() {
		
	}

	public void dormir() {
		
		System.out.println("Aluno dormindo 9hrs");
		super.dormir();
		
	}
	
	public int dormir() throws Exception {		
		System.out.println("Aluno dormindo 9hrs");
		super.dormir();
		return 0;
		
	}

