package br.atos.acelera.dados;

import java.util.Date;

public class Professor extends Funcionario {
	
	public String materia;
	
	
	public Professor (String aNome, short aIdade, Date aNascimento, String aMateria) {
		this.nome = aNome;
		this.idade = aIdade;
		this.nascimento = aNascimento;
		this.materia = aMateria;
	}
	
	
	void lecionar() {
		
	}

	public void dormir() {
		System.out.println("Professor dormindo 5hrs");
		
	}

}
